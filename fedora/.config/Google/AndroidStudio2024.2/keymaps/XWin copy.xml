<keymap version="1" name="XWin copy" parent="Default for XWin">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl insert" />
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="CopyPaths" />
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="EditorNextWordWithSelection" />
  <action id="OpenInlineChatAction">
    <keyboard-shortcut first-keystroke="shift ctrl g" />
  </action>
  <action id="UpdateRunningApplication" />
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="ctrl alt f10" />
  </action>
  <action id="copilot.applyInlays">
    <keyboard-shortcut first-keystroke="shift tab" />
  </action>
  <action id="copilot.applyInlaysNextWord">
    <keyboard-shortcut first-keystroke="shift ctrl right" />
  </action>
  <action id="copilot.disposeInlays" />
</keymap>